{
  "automock": false,
  "moduleFileExtensions": [
    "js"
  ],
  "testRegex": "lib/es6_global/test/.+test\\.js$",
  "transformIgnorePatterns": [
    "<rootDir>/node_modules/(?!(.*bs-)|reason-react|immutable-re|wonder-commonlib|wonder.js)"
  ],
  "setupTestFrameworkScriptFile": "<rootDir>/test/helper/jest/specHelper.js",
  "notify": false,
  "verbose": true,
  "collectCoverage": true,
  "coverageDirectory": "<rootDir>/test/coverage/",
  "coveragePathIgnorePatterns": [
    "<rootDir>/lib/es6_global/src/.+/Director.*",
    "<rootDir>/lib/es6_global/src/utils/",
    "<rootDir>/lib/es6_global/src/exception/",
    "<rootDir>/lib/es6_global/src/external/",
    "<rootDir>/lib/es6_global/src/ui/store/",
    "<rootDir>/lib/es6_global/src/redux/",
    "<rootDir>/lib/es6_global/src/config/",
    "<rootDir>/lib/es6_global/src/index.js",
    "<rootDir>/lib/es6_global/src/ui/app.js",
    "<rootDir>/lib/es6_global/src/component/mainEditor/ui/mainEditor.js",
    "<rootDir>/lib/es6_global/src/definition/"
  ],
  "collectCoverageFrom": [
    "lib/es6_global/src/**/*.{js}",
    "!**/node_modules/**",
    "!**/vendor/**"
  ]
}